{
  "expo": {
    "name": "booking-homestay-app",
    "slug": "booking-homestay-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "homiesstay",
    "deepLinks": [
  "homiesstay://payment-result"
],
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.homiesstayapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"]
    },
    "intentFilters": [
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": {
          "scheme": "homiesstay"
        },
        "category": ["BROWSABLE", "DEFAULT"]
      },
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": {
          "scheme": "https",
          "host": "homiesStay.com",
          "pathPrefix": "/"
        },
        "category": ["BROWSABLE", "DEFAULT"]
      }
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
